// Word Ladder Graph
digraph {
	rankdir=LR
	node [fontname=Arial shape=box style="rounded,filled"]
	suit [label=suit fillcolor=lightgreen]
	buds [label=buds fillcolor=pink]
	slit [label=slit fillcolor=lightgray]
	suit -> slit [style=dashed]
	skit [label=skit fillcolor=lightgray]
	suit -> skit [style=dashed]
	suet [label=suet fillcolor=lightgray]
	suit -> suet [style=dashed]
	quit [label=quit fillcolor=lightgray]
	suit -> quit [style=dashed]
	spit [label=spit fillcolor=lightgray]
	suit -> spit [style=dashed]
}
